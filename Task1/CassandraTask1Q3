create index on songspace.songs (Album);

Select Genre.characteristics from songspace.songs where Album = {name: 'The Nightfly', artist: 'Donald Fagen', genre: 'Fusion'} ALLOW FILTERING;

Select Genre.characteristics from songspace.songs where Album = {name: 'Mothership', artist: 'Led Zepplin', genre: 'Classic Rock'} ALLOW FILTERING;
